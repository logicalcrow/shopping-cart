{
    "name": "logicalcrow/shopping-cart",
    "description": "Laravel ShoppingCart",
    "keywords": [
        "laravel",
        "Shopping",
        "ShoppingCart"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Giovani Cespedes",
            "email": "giovanni@logicalcrow.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8|^9|^10|^11",
        "illuminate/session": "^8|^9|^10|^11",
        "illuminate/events": "^8|^9|^10|^11",
        "nesbot/carbon": "2.71.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^8.15.0"
    },
    "autoload": {
        "psr-4": {
            "Logicalcrow\\ShoppingCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Logicalcrow\\ShoppingCart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Logicalcrow\\ShoppingCart\\ShoppingCartServiceProvider"
            ],
            "aliases": {
                "Cart": "Logicalcrow\\ShoppingCart\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

